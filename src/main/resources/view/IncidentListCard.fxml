<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<HBox id="cardPane" fx:id="cardPane" fillHeight="false" prefHeight="150.0" prefWidth="150.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <GridPane prefHeight="150.0" HBox.hgrow="ALWAYS">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
       <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
     </columnConstraints>
     <rowConstraints>
       <RowConstraints minHeight="10.0" prefHeight="10.0" vgrow="SOMETIMES" />
       <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
         <HBox GridPane.halignment="LEFT" GridPane.valignment="TOP">
            <children>
                   <Label fx:id="id" styleClass="cell_big_label" text="\\$id" HBox.hgrow="ALWAYS">
                  <padding>
                     <Insets left="2.0" top="5.0" />
                  </padding>
                   </Label>
                   <Label fx:id="incidentId" styleClass="cell_big_label" text="\\$incidentId" underline="true" wrapText="true">
                  <padding>
                     <Insets top="5.0" />
                  </padding>
               </Label>
                  <Label fx:id="status" alignment="TOP_LEFT" styleClass="cell_small_label" text="\$status">
                  <HBox.margin>
                     <Insets />
                  </HBox.margin>
                  <padding>
                     <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </padding>
               </Label>
            </children>
         </HBox>
         <GridPane prefHeight="100.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
           <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
           </columnConstraints>
           <rowConstraints>
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
           </rowConstraints>
            <children>
                  <Label fx:id="operator" alignment="CENTER_RIGHT" prefHeight="20.0" styleClass="cell_small_label" text="\$operator" textAlignment="RIGHT" GridPane.halignment="RIGHT" GridPane.valignment="TOP">
                  <GridPane.margin>
                     <Insets right="5.0" />
                  </GridPane.margin>
               </Label>
                  <Label fx:id="incidentLocation" alignment="CENTER_RIGHT" prefHeight="20.0" styleClass="cell_small_label" text="\$incidentLocation" textAlignment="RIGHT" GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="TOP">
                  <GridPane.margin>
                     <Insets right="5.0" />
                  </GridPane.margin>
               </Label>
                    <Label fx:id="vehicle" alignment="CENTER_RIGHT" prefHeight="20.0" styleClass="cell_small_label" text="\$callerNumber" textAlignment="RIGHT" GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="TOP">
                    <GridPane.margin>
                        <Insets right="5.0" />
                    </GridPane.margin>
                </Label>
                  <Label fx:id="callerNumber" alignment="CENTER_RIGHT" prefHeight="20.0" styleClass="cell_small_label" text="\$callerNumber" textAlignment="RIGHT" GridPane.halignment="RIGHT" GridPane.rowIndex="3" GridPane.valignment="TOP">
                  <GridPane.margin>
                     <Insets right="5.0" />
                  </GridPane.margin>
               </Label>
            </children>
         </GridPane>
            <Label fx:id="dateTime" alignment="TOP_RIGHT" contentDisplay="RIGHT" nodeOrientation="LEFT_TO_RIGHT" styleClass="cell_small_label" text="\$dateTime" textAlignment="RIGHT" wrapText="true" GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.valignment="TOP">
            <GridPane.margin>
               <Insets right="5.0" top="5.0" />
            </GridPane.margin>
         </Label>
            <Label fx:id="description" styleClass="cell_small_label" text="\$description" wrapText="true" GridPane.halignment="LEFT" GridPane.rowIndex="1" GridPane.valignment="TOP">
            <GridPane.margin>
               <Insets />
            </GridPane.margin>
         </Label>
      </children>
   </GridPane>
</HBox>
